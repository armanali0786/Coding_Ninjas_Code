
public class Solution{  
    
    public static int duplicateNumber(int arr[]) {
    	//Your code goes here
        int dpl =0;
        int n = arr.length;
        for(int i=0; i<n; i++)
        {
            for(int j =i+1 ; j<n; j++)
            {
                if(arr[i] == arr[j])
                {
                    return arr[i];
                }
            }
        }
        return -1;
    }
}
